Config { font = "xft:monospace-9:bold"
       , bgColor = "black"
       , fgColor = "grey"
       , position = TopW L 90
       , lowerOnStart = False
       , commands = [ Run Network "wlp3s0" ["-L", "0", "-H", "32", "--normal", "green", "--high", "red"] 10
                    , Run Cpu ["-L","3","-H","50","--normal","green","--high","red"] 10
		    , Run Com "cat" ["/sys/devices/platform/applesmc.768/temp9_input"] "temp" 10
		    , Run Com "cat" ["/sys/devices/platform/applesmc.768/fan1_output"] "fan" 10
                    , Run Memory ["-t","Mem: <used>MB(<usedratio>%)"] 10
                    , Run Com "date" ["+\"%_m/%_d(%a) %H:%M\""] "jadate" 100
                    , Run Battery ["-t", "Bat: <left>%(<timeleft>)", "-L", "15", "-H", "70", "--high", "green", "--low", "red"] 100
                    , Run StdinReader
                    ]
       , sepChar = "%"
       , alignSep = "}{"
       , template = "%StdinReader% }{ %cpu%(%temp%â„ƒ) | Fan: %fan% | %memory% | %wlp3s0% | %battery%    <fc=#ee9a00>%jadate%</fc>"
       }
