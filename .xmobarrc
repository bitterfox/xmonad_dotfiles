Config { font = "xft:monospace-9:bold,Symbola-9:bold"
       , bgColor = "#4E4B42"
       , fgColor = "#D9D3BA"
       , position = TopW L 90
       , lowerOnStart = False
       , commands = [ 
                      Run Cpu ["-L","3","-H","50","--normal","#3BA99F","--high","#CC654C"] 10
		    , Run Com "sh" ["-c", "lscpu | grep 'CPU MHz' | grep -o '[1-9][0-9]*'"] "freq" 10
		    , Run Com "sh" [".xmonad/cpu_temp.sh"] "temp" 10
		    , Run Com "cat" ["/sys/devices/platform/applesmc.768/fan1_output"] "fan" 10
                    , Run Memory ["-t","Mem: <used>MB(<usedratio>%)"] 10
		    , Run DynNetwork ["-t", "‚¨áÔ∏è<rx>KB|‚¨ÜÔ∏è<tx>KB",  "-L", "0", "-H", "32", "--normal", "#3BA99F", "--high", "#CC654C"] 10
                    , Run Battery ["-t", "üîã<left>%(<timeleft>)", "-L", "15", "-H", "70", "--high", "#3BA99F", "--low", "#CC654C"] 100
		    , Run Com "sh" [".xmonad/audio_get.sh"] "volume" 3
		    , Run Com "sh" [".xmonad/bright_get.sh"] "brightness" 3
		    , Run Com "date" ["+%_m/%_d(%a) %H:%M"] "jadate" 100
                    , Run StdinReader
                    ]
       , sepChar = "%"
       , alignSep = "}{"
       , template = "%StdinReader% }{ %cpu% ‚àø%freq%MHz üå°%temp%‚ÑÉ üåÄ%fan% | %memory% | üì∂%dynnetwork% | %battery% | %volume% | ‚òÄ%brightness%    <fc=#cc8c4c>%jadate%</fc>"
       }
